<idea-plugin>
    <id>tk.weiba.idea.plugin.mysql</id>
    <name>Mysql-Log</name>
    <vendor email="xiaoweiba1028@gmail.com" url="https://github.com/xweiba/mysql-log-plugin">小尾巴</vendor>

    <description><![CDATA[
      Mysql Log Idea 插件<br>
      <em>1. 基于Java Agent模式加载</em><br>
      <em>2. 代理ClientPreparedStatement实现log输出</em><br>
      <em>3. 感谢小傅哥</em><br>]]></description>

    <depends>com.intellij.modules.java</depends>

    <extensions defaultExtensionNs="com.intellij">
        <!-- Add your extensions here -->
        <java.programPatcher implementation="tk.weiba.idea.plugin.mysql.AddMysqlAgentRunConfigByIdea" order="last"/>
    </extensions>

</idea-plugin>
